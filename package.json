{
  "name": "onemodel",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "main": "./dist/onemodel.cjs",
  "module": "./dist/onemodel.common.js",
  "types": "./index.d.ts",
  "exports": {
    "types": "./index.d.ts",
    "import": "./dist/onemodel.js",
    "require": "./dist/onemodel.cjs"
  },
  "scripts": {
    "clean": "del-cli --force ./dist",
    "build": "tsc && vite build --config vite-browser.config.ts && vite build --config vite-node.config.ts",
    "build.watch": "concurrently \"vite --watch build --config vite-browser.config.ts\" \"vite --watch build --config vite-node.config.ts\"",
    "test.client": "vite --config vite-browser.config.ts",
    "test.server": "node --trace-warnings src/server",
    "dev": "concurrently \"npm run build.watch\" \"npm run test.server\" \"npm run test.client\""
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "eslint": "^8.33.0",
    "events": "^3.3.0",
    "express": "*",
    "mongodb": "*",
    "mongodb-memory-server": "*",
    "typescript": "^4.9.4",
    "vite": "^4.0.0"
  }
}
